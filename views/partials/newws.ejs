<form method="post" class="col-md-8">
    <div class="form-group">
        <label for="key">Name</label>
        <input class="form-control" type="text" name="key" id="key" placeholder="Workspace Name">
    </div>
    <div class="form-group">
        <label for="workspace">Folder</label>
        <select class="form-control" id="workspace" name="workspace">
            <% ws.forEach(name => { %>
                <option value="<%= name %>"><%= name %></option>
            <% }); %>
        </select>
    </div>
    <div class="form-group">
        <label for="password">Username</label>
        <input class="form-control" type="text" name="username" id="username" placeholder="Workspace Username">
    </div>
    <div class="form-group">
        <label for="password">Password</label>
        <input class="form-control" type="password" name="password" id="password" placeholder="Workspace Password">
    </div>
    <button type="submit" class="btn btn-primary my-2">Submit</button>
</form>